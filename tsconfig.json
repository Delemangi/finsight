{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@api/*": [
        "./src/api/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@auth/*": [
        "./src/auth/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/index.rn.d.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
